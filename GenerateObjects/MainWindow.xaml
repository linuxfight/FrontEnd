<Window x:Class="GenerateObjects.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenerateObjects"
        mc:Ignorable="d"
        Title="GenerateObjects v1.0" Height="350" Width="800"
        FocusManager.FocusedElement="{Binding ElementName=MyCanvas}">
    <Canvas Focusable="True" Name="MyCanvas" MouseLeftButtonDown="MyCanvas_MouseLeftButtonDown" Background="AliceBlue" MouseRightButtonDown="MyCanvas_MouseRightButtonDown" MouseRightButtonUp="MyCanvas_MouseRightButtonUp" AllowDrop="True" MouseMove="MyCanvas_MouseMove">
        
    </Canvas>
</Window>
