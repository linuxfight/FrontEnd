<Window x:Class="MultiThreading.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiThreading"
        mc:Ignorable="d"
        Title="Multi Threading" Height="300" Width="400">
    <Grid>
        <StackPanel Width="200" Height="150">
            <Label Content="Multi Threading" HorizontalAlignment="Center" Margin="10"/>
            <Button Height="40" Content="Click Me" Margin="10" Click="Button_Click"/>
            <Label Name="lblResult" Content="RESULT" HorizontalAlignment="Center" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
